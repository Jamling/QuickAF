<cn.ieclipse.af.view.recycle.SwipeMenuLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!--支持侧滑内容此处id必须smContentView -->
    <include
        android:id="@+id/smContentView"
        layout="@layout/sample_list_item_news"/>

    <!--支持侧滑菜单此处id必须smMenuView -->
    <include
        android:id="@id/smMenuView"
        layout="@layout/sample_list_item_news_menu"/>

</cn.ieclipse.af.view.recycle.SwipeMenuLayout>